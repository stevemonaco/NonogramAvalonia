<Application
  x:Class="NonogramAvalonia.App"
  xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:con="using:NonogramAvalonia.Converters"
  xmlns:local="using:NonogramAvalonia"
  RequestedThemeVariant="Dark">
  <Application.DataTemplates>
    <local:ViewLocator />
  </Application.DataTemplates>

  <Application.Styles>
    <FluentTheme />
  </Application.Styles>

  <Application.Resources>
    <con:EnumToBooleanConverter x:Key="EnumToBooleanConverter" />
    <con:TimeSpanToTimerStringConverter x:Key="TimeSpanToTimerStringConverter" />
  </Application.Resources>
</Application>
